{
  "dashboard": {
    "id": null,
    "title": "PTE-QR System Dashboard",
    "tags": ["pte-qr", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pte_qr_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "xAxes": [
          {
            "type": "time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "HTTP Request Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(pte_qr_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(pte_qr_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "QR Codes Generated",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pte_qr_codes_generated_total[5m])",
            "legendFormat": "{{doc_uid}} {{revision}}"
          }
        ],
        "yAxes": [
          {
            "label": "QR Codes/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Document Status Checks",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pte_qr_document_status_checks_total[5m])",
            "legendFormat": "{{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Checks/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "ENOVIA API Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pte_qr_enovia_requests_total[5m])",
            "legendFormat": "{{endpoint}} {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pte_qr_cache_hits_total[5m]) / (rate(pte_qr_cache_hits_total[5m]) + rate(pte_qr_cache_misses_total[5m]))",
            "legendFormat": "Hit Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Hit Rate",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "pte_qr_cpu_percent",
            "legendFormat": "CPU %"
          },
          {
            "expr": "pte_qr_memory_percent",
            "legendFormat": "Memory %"
          },
          {
            "expr": "pte_qr_disk_percent",
            "legendFormat": "Disk %"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Active Connections",
        "type": "singlestat",
        "targets": [
          {
            "expr": "pte_qr_active_connections",
            "legendFormat": "Active"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 9,
        "title": "Database Connections",
        "type": "singlestat",
        "targets": [
          {
            "expr": "pte_qr_database_connections",
            "legendFormat": "DB"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Redis Connections",
        "type": "singlestat",
        "targets": [
          {
            "expr": "pte_qr_redis_connections",
            "legendFormat": "Redis"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "PDF Processing Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pte_qr_pdf_stamping_operations_total[5m])",
            "legendFormat": "{{operation_type}} {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 36
        }
      },
      {
        "id": 12,
        "title": "Authentication Attempts",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pte_qr_auth_attempts_total[5m])",
            "legendFormat": "{{method}} {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Attempts/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 36
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 1
  }
}
